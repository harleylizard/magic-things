{
  "multipart": [
    {
      "apply": {
        "model": "magic-things:block/fouled_growth_top",
        "uvlock": true
      },
      "when": {
        "OR": [
          { "y_side": 1 },
          { "y_side": 3 }
        ]
      }
    },
    {
      "apply": {
        "model": "magic-things:block/fouled_growth_bottom",
        "uvlock": true
      },
      "when": {
        "OR": [
          { "y_side": 2 },
          { "y_side": 3 }
        ]
      }
    }
  ]
}